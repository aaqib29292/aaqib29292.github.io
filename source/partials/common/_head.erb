<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Aaqib Parvez's Portfolio" %></title>

    <!-- adding bootstrap -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

    <!-- adding google fonts -->
    <link href='//fonts.googleapis.com/css?family=Lato:300,400' rel='stylesheet' type='text/css'>

    <link href='//fonts.googleapis.com/css?family=Work+Sans:400,300,700|Open+Sans:400italic,300italic' rel='stylesheet' type='text/css'>

    <!-- adding font awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">

    <!-- adding wow.class -->
    <link rel="stylesheet" href="stylesheets/animate.css">

    <!-- adding animsition.css -->
    <link rel="stylesheet" href="stylesheets/animsition.css">

    <!-- adding Elastic Grid Plugin CSS -->
    <link rel="stylesheet" href="stylesheets/demo.css">
    <link rel="stylesheet" href="stylesheets/elastic_grid.css">

    <%= stylesheet_link_tag :site %>
    <%= javascript_include_tag :all%>

    <!-- adding Fullscreen Overlay Navigation -->
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="../../javascripts/home.js"></script>

    <!-- adding wow.js -->
    <script src="../../javascripts/wow.min.js"></script>

    <!-- adding animsition effect -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="../../javascripts/animsition.min.js"></script>


    <script src="../../javascripts/typeit.min.js"></script>
    <!-- Elastic Grid Plugin JS -->
    <script src="../../javascripts/modernizr.custom.js"></script>
    <script src="../../javascripts/classie.js"></script>
    <script src="../../javascripts/elastic_grid.min.js"></script>
    <script src="../../javascripts/works.js"></script>

  </head>

  <body class="<%= page_classes %>">
